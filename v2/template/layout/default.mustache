<!DOCTYPE html>
<!--[if lt IE 7]> <html class="no-js lt-ie9 lt-ie8 lt-ie7"> <![endif]-->
<!--[if IE 7]> <html class="no-js lt-ie9 lt-ie8"> <![endif]-->
<!--[if IE 8]> <html class="no-js lt-ie9"> <![endif]-->
<!--[if gt IE 8]><!--> <html class="no-js"> <!--<![endif]-->
<head>
    <title>{{title}}</title>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8"/>
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="robots" content="noyaca"/>

    <script type="text/javascript">
        window.htmlStartTime = new Date().getTime();
        document.documentElement.className = document.documentElement.className.replace("no-js","js");
    </script>

    {{!> meta}}

    <link rel="shortcut icon" href="/favicon.ico"/>
    <link rel="apple-touch-icon" sizes="57x57" href="/apple-touch-icon.png">
    <link rel="apple-touch-icon" sizes="114x114" href="/apple-touch-icon.png">
    <link rel="apple-touch-icon" sizes="72x72" href="/apple-touch-icon.png">
    <link rel="apple-touch-icon" sizes="144x144" href="/apple-touch-icon.png">

    <meta name="viewport" content="width=1000" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <meta name="HandheldFriendly" content="true" />

    {{> partial/style }}
    {{!> headJavascript }}
    {{!> relLink }}
    {{!> googleAnalytics }}
    {{!> kissMetrics }}
    {{!> metaOg }}
</head>

<body class="{{bodyClassAttribute}}" data-template="{{bodyDataAttribute}}" data-id="{{id}}"{{#config.debug}} data-debug=true{{/config.debug}}>
    {{!> config }}

    {{!> adfoxBackground }}

    <div class="wrapper{{#wrapperClass}} {{wrapperClass}}{{/wrapperClass}}"{{#wrapperItemAttribute}} {{wrapperItemAttribute}}{{/wrapperItemAttribute}}>
        {{#header}}
        <header class="header">
            {{> partial/header }}
        </header><!--/ Шапка-->
        {{/header}}

        <div class="content clearfix">
            {{!> contentHead }}
            {{#content }}
                {{> content }}
            {{/content }}
        </div><!--/ Контент-->
    </div><!--/ Шаблон -->

    {{> partial/footer }}

    <a id="upper" class="upper" href="#">Наверх</a>

    {{!> userbar }}
    {{!> surveybar }}
    {{!> auth }}
    {{!> regionSelection }}

    <div style="position:absolute; height: 0; top:0; z-index:-1;">
        {{!> bodyJavascript }}
        {{!> innerJavascript }}
        {{!> yandexMetrika }}
        {{!> advanceSeoCounter }}
        {{!> myThings }}
        {{!> adriver }}
        {{!> partnerCounter }}
        {{> partial/bodyJavascript }}

        {{#config.analytics.enabled}}
            <div id="adblenderCommon" class="jsanalytics"></div>
        {{/config.analytics.enabled}}
    </div>
</body>
</html>
