		<!-- листинг товаров  -->
        {{#productBlock}}
        <div class="js-productList-container catalogList" data-url="{{url}}" data-value="{{&dataValue}}">
            {{#products}}
        	<a href="{{url}}" class="catalogList_item">
    			<div class="img"><img src="{{image}}" /></div>

        		<div class="catalogList_desc">
	        		<span href="{{url}}" class="name">{{name}}</span>

	        		<div class="reviews">
                        {{#rating}}
                            {{>partial/rating/star-list}}
                        {{/rating}}
                        <span class="reviews_count">{{rating.reviewCount}}</span>
	                </div>

	        		<div class="price">
                        {{#oldPrice}}<div class="price_old">{{shownOldPrice}} <span class="rubl">p</span></div>{{/oldPrice}}
                        {{#price}}<div class="price_now">{{shownPrice}} <span class="rubl">p</span></div>{{/price}}
	        		</div>
	        	</div>
        	</a>
            {{/products}}
        </div>
        <!--/ листинг товаров  -->

        <a href="" class="js-productList-more catalogMore btnStyleGrey" data-limit="{{&limit}}" data-filter-selector="" data-container-selector=".js-productList-container">Показать еще {{limit}}</a>
