<div class="content">
    <div class="contentBox960">
        <div class="catalogHead clearfix">
        	<div class="catalogHead_title">Бренды и параметры</div>

        	<a class="catalogHead_btn btnStyleGrey" href=""><span class="arrow">Еще параметры</span></a>
        </div>

        <div class="filter clearfix">
        	<div class="filterBox">
	        	<ul class="filterTags clearfix">
	        		<li class="filterTags_item">
	        			<a href="" class="filterTags_link"><span class="filterTags_text">Apple</span></a>
	        			<a href="" class="filterTags_link"><span class="filterTags_text">Samsung</span></a>
	        			<a href="" class="filterTags_link"><span class="filterTags_text">Explay</span></a>
	        			<a href="" class="filterTags_link"><span class="filterTags_text">Apple</span></a>
	        		</li>

	        		<li class="filterTags_item">
	        			<a href="" class="filterTags_link"><span class="filterTags_text">для дома</span></a>
	        			<a href="" class="filterTags_link"><span class="filterTags_text">для дачи</span></a>
	        			<a href="" class="filterTags_link"><span class="filterTags_text">для того</span></a>
	        			<a href="" class="filterTags_link"><span class="filterTags_text">для этого</span></a>
	        		</li>
	        	</ul>

	        	<ul class="filterChecked">
			        <li class="filterChecked_item filterChecked_item-title">Бренд</li>

	                <li class="filterChecked_item filterChecked_item-param">
	                    <a href="" class="delete">Huawei</a>
	                </li>

	                <li class="filterChecked_item filterChecked_item-param">
	                    <a href="" class="delete">Apple</a>
	                </li>

	                <li class="filterChecked_item filterChecked_item-clear"><a class="link" href=""><span class="text">Очистить все</span></a></li>
			    </ul>
			</div>

        	<script>
				$(function() {
					$( "#slider-range" ).slider({
						range: true,
						min: 0,
						max: 500,
						values: [ 0, 500 ]
					});
				});
			</script>

			<div class="priceRange">

				<span class="priceRange_title">Цена</span>

				<input type="text" class="priceRange_amount" />

				<div class="priceRange_slider" id="slider-range"></div>

				<input type="text" class="priceRange_amount" />

				<span class="priceRange_rubl rubl">p</span>
			</div>

			<ul class="sorting sortingFull">
				<li class="sorting_title">Сортировать</li>

				<li class="sorting_item sorting_item-active">Автоматически</li>
				<li class="sorting_item">Хиты продаж</li>
				<li class="sorting_item"><span class="top">По цене</span></li>
				<li class="sorting_item"><span class="drop">По цене</span></li>
			</ul>

			<div class="sorting sortingAbb">
				<span class="sorting_title">Сортировать</span>

				<div class="sortingSelect">Автоматически</div>

				<ul class="sortingList">
					<li class="sortingList_item">Автоматически</li>
					<li class="sortingList_item">Хиты продаж</li>
					<li class="sortingList_item"><span class="top">По цене</span></li>
					<li class="sortingList_item"><span class="drop">По цене</span></li>
				</ul>
			</div>
        </div>

        {{#productBlock}}
        <div class="catalogList">
            {{#products}}
        	<a href="{{url}}" class="catalogList_item">
    			<div class="img"><img src="{{image}}" /></div>

        		<div class="catalogList_desc">
	        		<span href="{{url}}" class="name">{{name}}</span>

	        		<div class="reviews">
                        {{#rating}}
                            {{>partial/rating/star-list}}
                        {{/rating}}
                        <span class="reviews_count">{{rating.reviewCount}}</span>
	                </div>

	        		<div class="price">
                        {{#oldPrice}}<div class="price_old">{{shownOldPrice}} <span class="rubl">p</span></div>{{/oldPrice}}
                        {{#price}}<div class="price_now">{{shownPrice}} <span class="rubl">p</span></div>{{/price}}
	        		</div>
	        	</div>
        	</a>
            {{/products}}
        </div>
        {{/productBlock}}

        <a href="" class="catalogMore btnStyleGrey">Показать еще 20</a>
    </div>
</div>

